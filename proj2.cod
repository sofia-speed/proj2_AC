aly:", "\tPOP R9", "\tPOP R8", "\tPOP R7", "\tPOP R6", "\tPOP R5", "\tPOP R4", "\tPOP R3", "\tPOP R2", "\tPOP R1", "\tPOP R0", "", "\tRET", "", ";---------------------------------------;", ";  \t\tMultiplicacao Preco*Peso\t \t;", ";---------------------------------------;", "", "MultiplicacaoPesoPreco:", "\tPUSH R0", "\tPUSH R1", "\tPUSH R2", "\tPUSH R3", "\tPUSH R5", "\tPUSH R6", "\tPUSH R7", "\tPUSH R11", "\t; R8,R9 Unidade,decimais", "\t; preco * peso", "\t; peso", "\tMOV R0, PESO", "\tMOV R1, [R0]", "\t; preco", "\tMOV R0, [PRECO]", "\tMOV R2, [R0]", "", ";-----------------------------------------------------------;", ";\ta,bc * d,e = (a*d),(bc*d) + ((a*e)*10) + ((bc*e)/10)\t;", ";-----------------------------------------------------------;", "SeparacaoDeUnidades:", "", "\tMOV R0, R2\t\t; r3 = preco(pk)", "\tMOV\tR3, R1\t\t; r1 = peso(P) ", "", "\tMOV R6, 100", "\tMOV R5, 10", "\t", "\tMOD R0, R6\t\t; R0 - Preco, decimas (bc)", "\tMOD R1, R5\t\t; R1 - Peso, decimas (e)", "\t", "\tDIV R2, R6\t\t; R2 - Preco, unidades (a)", "\tDIV R3, R5\t\t; R3 - Peso, unidades (d)", "MultiplicacoesIndividuais:", "", "\tMOV R5, R0\t\t; R5 = R0 = bc", "\tMOV R6, R1\t\t; R6 = R1 = e", "\tMOV R7, R2\t\t; R7 = R2 = a", "\tMOV R11, R3\t\t; R11 = R3 = d", "", "\t; a*d\tR7", "\tMUL R7, R3\t\t; parte inteira", "\t; bc*d\tR11", "\tMUL R11, R0\t\t; parte decimal", "\t; a*e\tR6", "\tMUL R6, R2\t\t; parte decimal", "\t; bc*e\tR5", "\tMUL R5, R1\t\t; parte decimal", "", "TratamentoDeValores:", "", "\tMOV R0, 10", "", "\tMUL R6, R0\t\t\t\t; ajuste para centenas da parte fracionaria do peso", "\t\t\t\t\t\t\t; (a*e)*10 => R6", "\tDIV R5, R0\t\t\t\t; ajuste da multiplicacao de decimais", "\t\t\t\t\t\t\t; (bc*e)/10 => R5", "\t", "\tADD R6, R5\t\t\t\t; R6 + R5 => R6", "\tADD R6, R11\t\t\t\t; (bc*d) + R6 => R6", "\t\t\t\t\t\t\t; R6 -> parte decimal do resultado", "\t\t\t\t\t\t\t; R7 -> parte inteira do resultado", "\tMOV R0, 100", "\tCMP R6, R0", "\tJLT ContinuaAdicao\t; se r0 > 100 => 1€ ", "", "Ve